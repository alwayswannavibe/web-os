{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "@craco/craco": "^6.4.3",
    "@fortawesome/fontawesome-svg-core": "^1.3.0-beta2",
    "@fortawesome/free-solid-svg-icons": "^6.0.0-beta2",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@reduxjs/toolkit": "^1.7.1",
    "@sentry/react": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "emoji-picker-react": "^3.4.12",
    "framer-motion": "^5.5.5",
    "i18next": "^21.6.3",
    "re-resizable": "^6.9.1",
    "react": "^17.0.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.2",
    "react-i18next": "^11.15.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-toastify": "^8.1.0",
    "socket.io-client": "^4.4.0",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "profiling": "craco build -- --profile",
    "serve": "serve -s build",
    "test": "craco test",
    "test:coverage": "craco test --coverage --watchAll",
    "eject": "react-scripts eject",
    "lint:style": "npx stylelint \"src/**/*.css\"",
    "lint:style:fix": "npx stylelint \"src/**/*.css\" --fix",
    "lint:ts": "npx eslint \"./src\" --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write \"./src\"",
    "lint": "npm run lint:style && npm run lint:ts",
    "prepare": "husky install",
    "cypress:open": "cypress open"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/hooks/useDragNDrop/index.ts"
    ],
    "moduleNameMapper": {
      "^@Enums(.*)$": "<rootDir>/src/enums$1",
      "^@Components(.*)$": "<rootDir>/src/components$1",
      "^@Constants(.*)$": "<rootDir>/src/constants$1",
      "^@Pages(.*)$": "<rootDir>/src/pages$1",
      "^@Interfaces(.*)$": "<rootDir>/src/interfaces$1",
      "^@Types(.*)$": "<rootDir>/src/types$1",
      "^@Features(.*)$": "<rootDir>/src/features$1",
      "^@Hooks(.*)$": "<rootDir>/src/hooks$1",
      "^@Utils(.*)$": "<rootDir>/src/utils$1",
      "^@Backgrounds(.*)$": "<rootDir>/src/assets/images/backgrounds$1",
      "^@Icons(.*)$": "<rootDir>/src/assets/images/icons$1",
      "^@Sounds(.*)$": "<rootDir>/src/assets/sounds$1",
      "^@Apps(.*)$": "<rootDir>/src/apps$1",
      "^@Chat(.*)$": "<rootDir>/src/apps/Chat$1",
      "^@Calculator(.*)$": "<rootDir>/src/apps/Calculator$1",
      "^@Minesweeper(.*)$": "<rootDir>/src/apps/Minesweeper$1",
      "^@ToDo(.*)$": "<rootDir>/src/apps/ToDo$1",
      "^@Translate(.*)$": "<rootDir>/src/apps/Translate$1",
      "^@Simon(.*)$": "<rootDir>/src/apps/Simon$1",
      "^@Settings(.*)$": "<rootDir>/src/apps/Settings$1",
      "^@Terminal(.*)$": "<rootDir>/src/apps/Terminal$1"
    }
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.37",
    "@types/react-custom-scrollbars": "^4.0.9",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-mock-store": "^1.0.3",
    "@types/uuid": "^8.3.3",
    "craco-alias": "^3.0.1",
    "cypress": "^9.1.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-order-config-standard": "^0.1.3",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.5.4"
  }
}

    "stylelint-order": "^4.1.0",
    "stylelint-order-config-standard": "^0.1.3",
    "stylelint-prettier": "^1.2.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.20.10",
    "@types/react": "^17.0.3",
